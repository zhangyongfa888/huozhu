/* pages/route/routeList.wxss */

.parent {
  padding-bottom: 80rpx;
}

.title {
  margin: 10rpx;
  font-size: 30rpx;
  color: #343434;
}

.item {
  padding: 20rpx;
  border-bottom: 1rpx solid #dcdcdc;
}

.addroute {
  box-sizing: border-box;
  text-align: center;
  display: flex;
  flex-direction: row;
  justify-content: center;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  padding: 18rpx;
  background: #f78312;
  color: white;
  font-size: 30rpx;
  position: fixed;
  bottom: 0;
  width: 750rpx;
}
