/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.theme-text-color {
  color: #e67300;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.bg {
  display: flex;
  flex-direction: column;
  width: 750rpx;
  
  background: #f5f5f5;
}
